<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Etiquetas para documentos del cliente -->
        <record id="documents_tag_ctl" model="documents.tag" forcecreate="0">
            <field name="name">CTL</field>
            <field name="sequence">10</field>
            <field name="tooltip">Certificado de Tradición y Libertad</field>
        </record>

        <record id="documents_tag_rut" model="documents.tag" forcecreate="0">
            <field name="name">RUT</field>
            <field name="sequence">20</field>
            <field name="tooltip">Registro Único Tributario</field>
        </record>

        <record id="documents_tag_cedula_ciudadania" model="documents.tag" forcecreate="0">
            <field name="name">Cedula de Ciudadanía</field>
            <field name="sequence">30</field>
            <field name="tooltip">Cédula de Ciudadanía del titular</field>
        </record>

        <record id="documents_tag_pagare" model="documents.tag" forcecreate="0">
            <field name="name">Pagare</field>
            <field name="sequence">40</field>
            <field name="tooltip">Pagaré firmado por el cliente</field>
        </record>

        <record id="documents_tag_fotos_negocio" model="documents.tag" forcecreate="0">
            <field name="name">Fotos del Negocio</field>
            <field name="sequence">50</field>
            <field name="tooltip">Fotografías del establecimiento o negocio</field>
        </record>

        <record id="documents_tag_sale_credit_quota_application" model="documents.tag" forcecreate="0">
            <field name="name">CIFIN</field>
            <field name="sequence">60</field>
            <field name="tooltip">Documento Soporte de revisión en la CIFIN</field>
        </record>
    </data>
</odoo>

