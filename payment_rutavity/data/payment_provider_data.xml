<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="payment_provider_rutavity" model="payment.provider">
        <field name="name">Rutavity</field>
        <field name="image_128" type="base64" file="payment_rutavity/static/img/rutavity_payment_logo.png"/>
        <field name="module_id" ref="base.module_payment_rutavity"/>
        <field name="payment_method_ids" eval="[(6, 0, [ref('payment.payment_method_pse')])]"/>
        <field name="code">rutavity</field>
        <field name="state">test</field>
        <field name="inline_form_view_id" ref="inline_form"/>
        <field name="redirect_form_view_id" ref="redirect_form"/>
        <field name="allow_tokenization">False</field>
    </record>

</odoo>